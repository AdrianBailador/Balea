<Project>
  <PropertyGroup Label="SDK Versions">
    <NetStandardTargetVersion21>netstandard2.1</NetStandardTargetVersion21>
    <NetCoreTargetVersion>netcoreapp3.1</NetCoreTargetVersion>
    <LangVersion>preview</LangVersion>
  </PropertyGroup>

  <PropertyGroup Label="Package information">
    <PackageLicenseUrl>https://github.com/Xabaril/Volvoreta/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/xabaril/Volvoreta</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xabaril/Volvoreta</RepositoryUrl>
    <Authors>Xabaril Contributors</Authors>
    <Company>Xabaril</Company>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>true</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup Label="Volvoreta Versions">
    <Volvoreta>1.0.0$(VersionSuffix)</Volvoreta>
  </PropertyGroup>

  <PropertyGroup Label="Package Dependencies">
    <MicrosoftExtensionsOptions>3.1.0</MicrosoftExtensionsOptions>
    <MicrosoftEntityFrameworkCoreSqlServer>3.1.0</MicrosoftEntityFrameworkCoreSqlServer>
    <MicrosoftEntityFrameworkCoreDesign>3.1.0</MicrosoftEntityFrameworkCoreDesign>
    <MicrosoftEntityFrameworkCoreRelational>3.1.0</MicrosoftEntityFrameworkCoreRelational>
  </PropertyGroup>

  <PropertyGroup Label="Testing Dependencies">
    <MicrosoftNETTestSdk>16.4.0</MicrosoftNETTestSdk>
    <xunit>2.4.1</xunit>
    <xunitrunnervisualstudio>2.4.1</xunitrunnervisualstudio>
    <coverletcollector>1.2.0</coverletcollector>
    <AcheveTestHost>3.0.0</AcheveTestHost>
    <FluentAssertions>5.10.0</FluentAssertions>
    <MicrosoftAspNetCoreTestHost>3.1.1</MicrosoftAspNetCoreTestHost>
  </PropertyGroup>

  <PropertyGroup Label="CLI Tools Versions">

  </PropertyGroup>
  
</Project>